,"column":12}},{"generated":{"line":97,"column":23},"source":"component/DisplaySongList.js","original":{"line":52,"column":43}},{"generated":{"line":98,"column":0},"source":"component/DisplaySongList.js","original":{"line":52,"column":46}},{"generated":{"line":98,"column":4},"source":"component/DisplaySongList.js","original":{"line":52,"column":46}},{"generated":{"line":98,"column":13},"source":"component/DisplaySongList.js","original":{"line":52,"column":55}},{"generated":{"line":98,"column":15},"source":"component/DisplaySongList.js","original":{"line":52,"column":56}},{"generated":{"line":99,"column":0},"source":"component/DisplaySongList.js","original":{"line":52,"column":43}},{"generated":{"line":99,"column":11},"source":"component/DisplaySongList.js","original":{"line":52,"column":12}},{"generated":{"line":99,"column":12},"source":"component/DisplaySongList.js","original":{"line":33,"column":8}},{"generated":{"line":99,"column":13},"source":"component/DisplaySongList.js","original":{"line":32,"column":4}},{"generated":{"line":100,"column":0},"source":"component/DisplaySongList.js","original":{"line":55,"column":1}},{"name":"DisplaySongList","generated":{"line":102,"column":15},"source":"component/DisplaySongList.js","original":{"line":57,"column":15}}],"sources":{"component/DisplaySongList.js":"import React, { useContext, useState } from 'react';\r\nimport { Link } from 'react-router-dom';\r\nimport { Context } from '../Context';\r\n\r\nfunction DisplaySongList({ song }) {\r\n    const { \r\n        handleUpvotes, \r\n        handleDownvotes, \r\n        handleFavorited,\r\n        carts,\r\n        addToCart,\r\n        deleteFromCart,\r\n    } = useContext(Context);\r\n\r\n    function heartIcon() {\r\n        if (song.isFavorited) {\r\n            return <i onClick={() => handleFavorited(song.id)} className=\"ri-heart-fill favorite\"></i>\r\n        } else {\r\n            return <i onClick={() => handleFavorited(song.id)} className=\"ri-heart-line favorite\"></i>\r\n        }\r\n    }\r\n\r\n    function cartIcon() {\r\n        const moveToCart = carts.some(cart => cart.id === song.id)\r\n        if (moveToCart) {\r\n            return <i onClick={() => deleteFromCart(song.id)} className=\"ri-shopping-cart-fill cart-fill\"></i>\r\n        } else {\r\n            return <i onClick={() => addToCart(song)} className=\"ri-shopping-cart-line cart-line\"></i>\r\n        }\r\n    }\r\n\r\n    return (\r\n        <li className=\"list-items\">\r\n            <p\r\n                className=\"heart-icon-line\">{heartIcon()}\r\n            </p>\r\n            <div className=\"heading\">\r\n                <h2 className=\"title\">{song.title}</h2>\r\n                <p className=\"author\">{song.author}</p>\r\n            </div>\r\n            <div className=\"upvotes\">\r\n                <span>{song.upvotes}\r\n                </span>\r\n                <button className=\"arrowup\" onClick={() => handleUpvotes(song.id)}> ↑ </button>\r\n            </div>\r\n            <div className=\"downvotes\">\r\n                <span>{song.downvotes}\r\n                </span>\r\n                <button className=\"arrowdown\" onClick={() => handleDownvotes(song.id)}> ↓ </button>\r\n            </div>\r\n            <div className=\"cart-icon-line\">{cartIcon()}</div>\r\n            <Link to={`/song/${song.id}`}> <p className=\"song-lyrics\">...</p></Link>\r\n        </li>\r\n    )\r\n}\r\n\r\nexport default DisplaySongList\r\n"},"lineCount":null}},"error":null,"hash":"53ea7175cf76daa91a9794e8ea3f9827","cacheData":{"env":{}}}