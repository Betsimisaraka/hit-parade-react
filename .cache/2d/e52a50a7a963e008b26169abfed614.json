nent/DisplaySongList.js","original":{"line":51,"column":20}},{"generated":{"line":88,"column":4},"source":"component/DisplaySongList.js","original":{"line":51,"column":20}},{"generated":{"line":88,"column":13},"source":"component/DisplaySongList.js","original":{"line":51,"column":29}},{"generated":{"line":88,"column":15},"source":"component/DisplaySongList.js","original":{"line":51,"column":30}},{"generated":{"line":89,"column":0},"source":"component/DisplaySongList.js","original":{"line":51,"column":12}},{"generated":{"line":89,"column":11},"source":"component/DisplaySongList.js","original":{"line":32,"column":8}},{"generated":{"line":89,"column":12},"source":"component/DisplaySongList.js","original":{"line":31,"column":4}},{"generated":{"line":90,"column":0},"source":"component/DisplaySongList.js","original":{"line":54,"column":1}},{"name":"DisplaySongList","generated":{"line":92,"column":15},"source":"component/DisplaySongList.js","original":{"line":56,"column":15}}],"sources":{"component/DisplaySongList.js":"import React, { useContext, useState } from 'react';\r\nimport { Context } from '../Context';\r\n\r\nfunction DisplaySongList({ song }) {\r\n    const { \r\n        handleUpvotes, \r\n        handleDownvotes, \r\n        handleFavorited,\r\n        carts,\r\n        addToCart,\r\n        deleteFromCart,\r\n    } = useContext(Context);\r\n\r\n    function heartIcon() {\r\n        if (song.isFavorited) {\r\n            return <i onClick={() => handleFavorited(song.id)} className=\"ri-heart-fill favorite\"></i>\r\n        } else {\r\n            return <i onClick={() => handleFavorited(song.id)} className=\"ri-heart-line favorite\"></i>\r\n        }\r\n    }\r\n\r\n    function cartIcon() {\r\n        const moveToCart = carts.some(cart => cart.id === song.id)\r\n        if (moveToCart) {\r\n            return <i onClick={() => deleteFromCart(song.id)} className=\"ri-shopping-cart-fill cart\"></i>\r\n        } else {\r\n            return <i onClick={() => addToCart(song)} className=\"ri-add-circle-line cart\"></i>\r\n        }\r\n    }\r\n\r\n    return (\r\n        <li className=\"list-items\">\r\n            <p\r\n                className=\"heart-icon-line\">{heartIcon()}\r\n            </p>\r\n            <div className=\"heading\">\r\n                <h2>{song.title}</h2>\r\n                <p>{song.author}</p>\r\n            </div>\r\n            <div className=\"upvotes\">\r\n                <span>{song.upvotes}\r\n                </span>\r\n                <button onClick={() => handleUpvotes(song.id)}> ↑ </button>\r\n            </div>\r\n            <div className=\"downvotes\">\r\n                <span>{song.downvotes}\r\n                </span>\r\n                <button onClick={() => handleDownvotes(song.id)}> ↓ </button>\r\n            </div>\r\n            <div className=\"cart-icon-line\">{cartIcon()}</div>\r\n            <button className=\"song-lyrics\">...</button>\r\n        </li>\r\n    )\r\n}\r\n\r\nexport default DisplaySongList\r\n"},"lineCount":null}},"error":null,"hash":"11b6184fd445f79ccb41db4d9f32e0a8","cacheData":{"env":{}}}